{
  "name": "growth_stars",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run serve & npm run format",
    "serve": "vue-cli-service serve",
    "pre": "vue-cli-service build --pre",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix --ext .js --ext .vue stars/*/src/",
    "format": "onchange 'stars/*/src/**/*.js' 'stars/*/src/**/*.vue' 'stars/*/src/**/*.scss' -- prettier --write {{changed}}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "stars/*/src/**/*.{js,vue}": [
      "eslint --ext stars/*/src/**/*.{js,vue}",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "growth-jsonp": "^1.0.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "node-sass": "^4.12.0",
    "onchange": "^6.1.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
